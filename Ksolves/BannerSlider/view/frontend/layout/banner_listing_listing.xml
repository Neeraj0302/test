<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    layout="1column"
    xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <head>
        <script src="Ksolves_BannerSlider::js/common.js" async="async" />
    </head>
    <body>
        <referenceBlock name="content">
            <block class="Ksolves\BannerSlider\Block\Listing"
                name="BannerSlider"
                template="Ksolves_BannerSlider::slider.phtml" />
        </referenceBlock>
    </body>
</page>






